int main() {
    int cnum = 3, charges[3];
    int amount = 100, dp[101];
    int valid = 1;
    int i = 0, j = 0;
    charges[0] = 7;
    charges[1] = 2;
    charges[2] = 5;
    
    if (valid == 0) {
        write(-1);
        return 0;
    }
   
    dp[0] = 0;
    i = 1;
    while (i < amount + 1) {
        dp[i] = amount + 1;
        i = i + 1;
    }

    i = 0;
    while (i < cnum) {
        int chg = charges[i];
        j = chg;
        while (j < amount + 1) {
            if (dp[j - chg] + 1 < dp[j]) {
                dp[j] = dp[j - chg] + 1;
            }
            j = j + 1;
        }
        i = i + 1;
    }

    if (dp[amount] > amount) {
        write(-1);
    } else {
        write(dp[amount]);
    }
    return 0;
}
